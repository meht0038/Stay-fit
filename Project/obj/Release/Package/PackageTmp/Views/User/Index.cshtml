        <form action="Index" method="post">
            <div class="login">
                <h3 for="modlgn_username" style="color:azure; font-size:25px">Calculate your BMI</h3>
                <div>

                    <div>

                        <br />
                        <fieldset class="input">
                            <p id="login-form-username" class="col-md-4">
                                <label for="modlgn_username">Height</label>
                                <input id="modlgn_username" type="text" placeholder="Enter Height in inch" name="Height" class="inputbox" size="18">
                            </p>
                            <p id="login-form-username" class="col-md-4">
                                <label for="modlgn_username">Weight</label>
                                <input id="modlgn_username" type="text" placeholder="Enter Weight in Kg" name="Weight" class="inputbox" size="18">
                            </p>
                            <br />
                            <div class="clear"> </div>
                            <br />
                            <input type="submit" style="float:left; color:azure; font-size:25px" name="Calcuate Your BMI" class="button" value="Calcuate Your BMI"><div class="clear"></div>

                            <br />
                            @if (Convert.ToString(Session["bmi"]) != "")
                            {

                                <p id="login-form-username">
                                    <label for="modlgn_username">Your BodyMassIndex </label>
                                    <input id="modlgn_username" type="text" value="@Convert.ToString(Session["bmi"])" name="bmi" class="inputbox" size="18">
                                </p>
                                Session["bmi"] = "";
                            }

                        </fieldset>
                    </div>
                </div>
            </div>
